<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="payment_mode_form" model="ir.ui.view">
        <field name="name">payment.mode.l10n_br_account_payment_brcobranca.form</field>
        <field name="model">account.payment.mode</field>
        <field name="priority">99</field>
        <field
            name="inherit_id"
            ref="l10n_br_account_payment_order.l10n_br_account_payment_mode_form"
        />
        <field name="arch" type="xml">
            <xpath expr="//page[@name='cnab_config']" position="after">
                <page name="config_brcobranca" string="Configuração BRCobrança">
                    <group name="main">
                        <field name="brcobranca_modelo" />
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
